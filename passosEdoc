ALTER TABLE `content` ADD `issn` VARCHAR(45) NOT NULL AFTER `magazine`, ADD `title` VARCHAR(545) NOT NULL AFTER `issn`, ADD `contrib` TEXT NOT NULL AFTER `title`, ADD `autor_notes` VARCHAR(545) NOT NULL AFTER `contrib`, ADD `pub_date` VARCHAR(545) NOT NULL AFTER `autor_notes`, ADD `abstract` TEXT NOT NULL AFTER `pub_date`, ADD `keywords` TEXT NOT NULL AFTER `abstract`, ADD `volume` VARCHAR(45) NOT NULL AFTER `keywords`, ADD `issue` VARCHAR(45) NOT NULL AFTER `volume`, ADD `fpage` VARCHAR(45) NOT NULL AFTER `issue`, ADD `lpage` VARCHAR(45) NOT NULL AFTER `fpage`, ADD `content_pt` LONGTEXT NOT NULL AFTER `lpage`, ADD `content_en` LONGTEXT NOT NULL AFTER `content_pt`, ADD `referencies` LONGTEXT NOT NULL AFTER `content_en`;




