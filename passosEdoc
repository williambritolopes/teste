"t2002\">\n           <label>Tabela 2</label>\n           <caption>\n             <h2>Variabilidade da frequência cardíaca no domínio tempo</h2>\n            </caption>\n            <table frame=\"hsides\" rules=\"groups\">\n             <colgroup width=\"14%\">\n                <col/>\n                <col/>\n                <col/>\n                <col/>\n                <col/>\n                <col/>\n                <col/>\n              </colgroup>\n             <thead>\n               <tr>\n                  <th align=\"left\" rowspan=\"2\">Variáveis</th>\n                 <th>Grupo 1</th>\n                  <th>Grupo 2</th>\n                  <th>Grupo 3</th>\n                  <th>Grupo 4</th>\n                  <th rowspan=\"2\">Valor de p*</th>\n                  <th rowspan=\"2\">Múltiplas comparações entre grupos<sup>†</sup></th>\n               </tr>\n               <tr>\n                  <th>(n=26)</th>\n                 <th>(n=41)</th>\n                 <th>(n=27)</th>\n                 <th>(n=32)</th>\n               </tr>\n             </thead>\n              <tbody>\n               <tr>\n                  <td>RR média, ms</td>\n                 <td align=\"center\">904,7 (81,3)</td>\n                  <td align=\"center\">911,8 (96,9)</td>\n                  <td align=\"center\">869,7 (122,1)</td>\n                 <td align=\"center\">915,7 (104,4)</td>\n                 <td align=\"center\">0,53</td>\n                  <td> </td>\n                </tr>\n               <tr>\n                  <td>DPNN, ms</td>\n                 <td align=\"center\">64,7 (19,4)</td>\n                 <td align=\"center\">56,3 (17,8)</td>\n                 <td align=\"center\">53,2 (22,5)</td>\n                 <td align=\"center\">47,2 (16,0)</td>\n                 <td align=\"center\">0,04</td>\n                  <td align=\"center\">1 e 4</td>\n               </tr>\n               <tr>\n                  <td>RQMDS, ms</td>\n                  <td align=\"center\"